{
  "presets": ["es2015", "stage-0"],
  "plugins": [
    "dev-expression",
    "transform-runtime",
    "transform-flow-strip-types"
  ],

  "env": {
    "cjs": {
      "presets": ["es2015-loose", "stage-0"],
      "plugins": ["add-module-exports"]
    },
    "es": {
      "presets": ["es2015-loose-native-modules", "stage-0"]
    }
  }
}
